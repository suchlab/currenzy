{
  "name": "currenzy",
  "version": "0.0.1",
  "description": "Get currencies exchange rates",
  "scripts": {
    "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs && sh ./scripts/build-packages.sh",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prepare": "npm run build",
    "test": "npm run build && jest --config jestconfig.json --passWithNoTests"
  },
  "author": "itaibo@hey.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.1"
  },
  "devDependencies": {
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "exchange-rates",
    "currencies"
  ],
  "homepage": "https://github.com/currenzy/currenzy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/currenzy/currenzy.git"
  },
  "bugs": {
    "url": "https://github.com/currenzy/currenzy/issues"
  }
}
